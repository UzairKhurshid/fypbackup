<!-- Container -->
<div style="min-height:200px;" class="profile-cover-wrap overlay-wrap">
    <div class="profile-cover-img" style="background-image:url('/dist/img/dashboard_back.jpg');"></div>
    <div class="bg-overlay bg-trans-dark-60"></div>
    <div class="container-fluid profile-cover-content py-50"></div>
</div>

<div class="container-fluid mt-xl-50 mt-sm-30 mt-15">
    {{#if error}}
        <div class="alert alert-success alert-dismissible fade show" role="alert">
            <span class="alert-icon"><i class="ni ni-like-2"></i></span>
            <span class="alert-text"><strong>Error!</strong> {{error}}</span>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">×</span>
            </button>
        </div>
    {{/if}}
    {{#if success}}
        <div class="alert alert-success alert-dismissible fade show" role="alert">
            <span class="alert-icon"><i class="ni ni-like-2"></i></span>
            <span class="alert-text"><strong>Success!</strong> {{success}}</span>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">×</span>
            </button>
        </div>
    {{/if}} 

    {{#if teacherLogin}}
        <!--  Teacher Row -->
        <div class="row">
            <div class="col-xl-12">
                <div class="hk-row">
                    <div class="col-lg-4">

                        <div style="min-height:265px" class="card border-0">
                            <div class="card-body w-100 d-flex justify-content-start pa-0">
                                <lottie-player src="https://assets8.lottiefiles.com/packages/lf20_M4H0kx.json"
                                               background="transparent" speed="1.5" style="width: auto; height:265px"
                                               loop
                                               autoplay></lottie-player>
                            </div>
                        </div>
                        <div style="min-height:561px" class="card">
                            <div class="card-header card-header-action">
                                <h6>Supervising Projects</h6>
                            </div>
                            <div class="card-body">
                                {{#if teacherData}}
                                    {{#each teacherData}}
                                        <div class="user-activity user-activity-sm dash-super">
                                            <div class="media">
                                                <div class="media-img-wrap">
                                                    <div class="avatar avatar-xs">
														<span class="avatar-text avatar-text-success rounded-circle">
																<span class="initial-wrap text-uppercase"><span>{{this.projectLetter}}</span></span>
														</span>
                                                    </div>
                                                </div>
                                                <div class="media-body">
                                                    <div>
                                            <span class="d-block mb-5"><span
                                                    class="font-weight-500 text-dark text-capitalize">{{this.projectName}}</span><span
                                                    class="pl-5">{{this.projectDescription}}</span></span>
                                                        <span class="d-block font-13">{{this.studentDepartment}}</span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="media">
                                                <div class="media-img-wrap">
                                                    <div class="avatar avatar-xs">
                                                        {{#if studentAvatar}}
                                                            <img src="data:image/jpeg;base64, {{this.studentAvatar}}"
                                                                 alt="user" class="avatar-img rounded-circle">
                                                        {{else}}
                                                            <span class="avatar-text avatar-text-info rounded-circle">
																<span class="initial-wrap text-uppercase"><span>{{this.studentLetter}}</span></span>
														</span>
                                                        {{/if}}

                                                    </div>
                                                </div>
                                                <div class="media-body">
                                                    <div>
                                            <span class="d-block mb-5"><span
                                                    class="font-weight-500 text-dark text-capitalize">{{this.studentName}}</span><span
                                                    class="pl-5">{{this.studentEmail}}</span></span>
                                                        <span class="d-block font-13">{{this.studentDepartment}}</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="dropdown-divider pb-20"></div>
                                    {{/each}}
                                    {{else}}
                                    <h5 class="display-5 font-weight-bold">You are not supervising any projects at this moment.</h5>
                                {{/if}}
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-8">
                        <div style="min-height:561px" class="card">
                            <div class="card-body pa-0">
                                <div class="table-wrap">
                                    <div class="table-responsive">
                                            {{#if teacherData}}
                                            <table class="table table-sm table-hover mb-0">
                                                <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>Project</th>
                                                    <th>Project Progress</th>
                                                    <th>Task</th>
                                                    <th>Description</th>
                                                    <th>Status</th>

                                                </tr>
                                                </thead>
                                            <tbody>
                                                {{#each teacherData}}
                                                    {{#each this.projectTasks}}
                                                        <tr>
                                                            <td>
                                                                <div class="media-img-wrap">
                                                                    <div class="avatar avatar-xs">
                                                          <span class="avatar-text avatar-text-blue rounded-circle">
																<span class="initial-wrap text-uppercase"><span> {{../this.projectLetter}}</span></span>
														</span>
                                                                    </div>
                                                                </div>
                                                            </td>
                                                            <td class="text-capitalize">{{../this.projectName}}</td>
                                                            <td>
                                                                <div class="progress-wrap lb-side-left mnw-125p">
                                                                    <div class="progress-lb-wrap">
                                                                        <label class="progress-label progress-text-{{../this.projectSemester}}  mnw-25p"></label>
                                                                        <div class="progress progress-bar-xs">
                                                                            <div class="progress-bar progress-bar-{{../this.projectSemester}}"
                                                                                 role="progressbar" aria-valuenow="70"
                                                                                 aria-valuemin="0"
                                                                                 aria-valuemax="100"></div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </td>
                                                            <td class="text-capitalize">{{this.taskName}}</td>
                                                            <td class="text-capitalize">{{this.taskDescription}}</td>
                                                            <td><span
                                                                    class="badge badge-status-{{this.status}} ">{{this.status}}</span>
                                                        </tr>
                                                    {{/each}}
                                                {{/each}}
                                            {{else}}
                                                <h5 class="display-5 font-weight-bold p-5">No proposed projects at this moment.</h5>
                                            {{/if}}
                                            </tbody>
                                            <script type="text/javascript">
                                                $('.badge-status-complete').addClass('badge-success');
                                                $('.badge-status-incomplete').addClass('badge-orange');
                                                $('.progress-text-6').html('50%');
                                                $('.progress-text-7').html('80%');
                                                $('.progress-text-8').html('100%');
                                                $('.progress-bar-6').addClass('bg-orange w-50');
                                                $('.progress-bar-7').addClass('bg-blue w-80');
                                                $('.progress-bar-8').addClass('bg-primary w-100');
                                            </script>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div style="min-height:265px" class="card overflow-hide border-0">
                            <div class="card-body pa-0">
                                <div id="owl_demo_1" class="owl-carousel dots-on-item owl-theme">
                                    <div class="fadeOut item img-background overlay-wrap"
                                         style="background-image:url(/dist/img/corona.jpg);">
                                        <div class="position-relative z-index-2 pa-20">
