                                            <div class="card border-0 bg-gradient-warning card-sm">
                                                <div class="card-body text-white">
                                                    <div class="d-flex justify-content-between mb-5">
                                                        <div>
                                                            <span class="d-block font-15 font-weight-500">Students</span>
                                                        </div>
                                                    </div>
                                                    <div>
                                                        <span class="d-block display-4 mb-5"><span class="counter-anim">{{adminData.studentsCount}}</span></span>
                                                        <small class="d-block">Number Of Students Using Portal </small>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="card border-0 bg-gradient-primary card-sm">
                                                <div class="card-body text-white">
                                                    <div class="d-flex justify-content-between mb-5">
                                                        <div>
                                                            <span class="d-block font-15  font-weight-500">Projects</span>
                                                        </div>
                                                    </div>
                                                    <div>
                                                        <span class="d-block display-4 mb-5"><span class="counter-anim">{{adminData.projectsCount}}</span></span>
                                                        <small class="d-block">Final Year Projects Submitted & On Going</small>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                        <div class="card">
                                            <div class="card-body pa-0">
                                                <div class="table-wrap">
                                                    <div class="table-responsive">
                                                        <table class="table table-hover mb-0">
                                                            <thead>
                                                            <tr>
                                                                <th>Register No</th>
                                                                <th>Name</th>
                                                                <th>Email</th>
                                                                <th>Role</th>
                                                                <th>Status</th>
                                                                <th>Action</th>
                                                            </tr>
                                                            </thead>
                                                            <tbody>
                                                            {{#if adminData.accounts}}
                                                                {{#each adminData.accounts}}
                                                                    <!-- <th class="text-center" colspan="6">Show All Students Here.</th> -->
                                                                    <tr>
                                                                        <td>
                                                                            {{#if this.regNo}}
                                                                                {{this.regNo}}
                                                                            {{else}}
                                                                                -
                                                                            {{/if}}
                                                                        </td>
                                                                        <td class="text-capitalize">{{this.name}}</td>
                                                                        <td>{{this.email}}</td>
                                                                        <td>{{this.role}}</td>
                                                                        <td><span class="badge badge-danger">{{this.status}}</span>
                                                                        </td>
                                                                        <td>
                                                                            <div class="btn-group" role="group"
                                                                                 aria-label="Basic example">
                                                                                <a style="margin-right:3px;"
                                                                                   class="btn btn-outline-orange btn-sm"
                                                                                   href="/adminAccount/update/{{this.id}}">Edit</a>
                                                                                <form onsubmit="return form_confirm(this);" method="POST"
                                                                                      action="/adminAccount/delete/{{this.id}}">
                                                                                    <input type="hidden" name="_csrf"
                                                                                           value="{{../csrfToken}}"
                                                                                           class="form-control">
                                                                                    <button class="btn btn-outline-danger btn-sm"
                                                                                            type="submit">Delete
                                                                                    </button>
                                                                                </form>
                                                                            </div>
                                                                        </td>
                                                                    </tr>
                                                                {{/each}}
                                                                <script>
                                                                    function form_confirm(form) {
                                                                        Swal.fire({
                                                                            title: 'Are you sure?',
                                                                            text: "You won't be able to revert this!",
                                                                            icon: 'warning',
                                                                            showCancelButton: true,
                                                                            confirmButtonText: 'Yes, delete it!'
                                                                        }).then((result) => {
                                                                            if (result.value) {
                                                                                Swal.fire(
                                                                                        'Deleted!',
                                                                                        'Your file has been deleted.',
                                                                                        'success'
                                                                                )
                                                                                return form.submit();
                                                                            }
                                                                        });
                                                                        return false;
                                                                    }
                                                                </script>
                                                              {{else}}
                                                                <td colspan="6" class="text-capitalize text-center">No Deactivated Users At This Moment.</td>
                                                            {{/if}}
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- / Teacher Row -->
    {{/if}}
</div>
<!-- /Container -->